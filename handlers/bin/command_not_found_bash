command_not_found_handle() {

    # do not run when inside Midnight Commander
    if [ -n "$MC_SID" ]; then
        return 127
    fi

    # run command-not-found program
    [ -x /usr/bin/python ] && [ -x /usr/bin/command-not-found ] && /usr/bin/python /usr/bin/command-not-found "$@" __REPO__ || return 127

}
