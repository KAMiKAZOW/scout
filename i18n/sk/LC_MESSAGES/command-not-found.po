#: handlers/bin/command-not-found:19
#, python-format
msgid "The program '%s' can be found in the following package(s):"
msgstr "Program '%s' sa nachádza v následujúcich balíkoch:"

#: handlers/bin/command-not-found:21
#, python-format
msgid "  * %s [ path: %s/%s, repository: %s ]"
msgstr "  * %s [ cesta: %s/%s, repozitár: %s ]"

#: handlers/bin/command-not-found:23
msgid "Try:"
msgstr "Skúste:"

#: handlers/bin/command-not-found:25
msgid "<selected_package>"
msgstr "<vybrané_balíky>"

#: handlers/bin/command-not-found:33
#, python-format
msgid "Program '%s' is present in package '%s', which is installed on your system."
msgstr "Program '%s' sa nachádza v balíku '%s', který už je v systéme nainštalovaný."

#: handlers/bin/command-not-found:35
#, python-format
msgid "Full path leading to '%s' is '%s/%s'."
msgstr "Plná cesta k programu '%s' je '%s/%s'."

#: handlers/bin/command-not-found:37
#, python-format
msgid ""
"Please check your $PATH or maybe program '%s' is intended to be run only by "
"root"
msgstr "Ověřte prosím obsah proměnné $PATH, nebo je možná program '%s' určen pouze pro běh pod uživatelem root."

